<layout label="Titel + Tekst links + Afbeelding rechts">
	<table class="w640" border="0" cellpadding="0" cellspacing="0" width="800">
		<tbody>
			<tr>
				<td class="w640" height="21" width="800" style="font-size: 21px; line-height: 21px;">&nbsp;</td>
			</tr>
			<tr>
				<td class="w640" width="800">
					<div class="article-title" align="left">
						<multiline label="Title"></multiline>
					</div>
				</td>
			</tr>
			<tr>
				<td class="w640" height="21" width="800" style="font-size: 21px; line-height: 21px;">&nbsp;</td>
			</tr>
			<tr>
				<td class="w640" width="800">
					<table class="w640" width="800" border="0" cellpadding="0" cellspacing="0">
						<tbody>
							<tr>
								<td width="470" class="w640">
									<div class="article-content">
										<multiline label="Description"><p></multiline>
									</div>
								</td>
								<td width="30" class="hide"></td>
								<td width="300" class="w640">
									<img src="img/spacer.gif" border="0" width="1" height="1">
									<img src="img/spacer.gif" border="0" width="1" height="1">
									<img src="img/spacer.gif" border="0" width="1" height="1">
									<img editable="true" src="" label="Image" class="w640" border="0" width="300">
								</td>
							</tr>
						</tbody>
					</table>
				</td>
			</tr>
			<tr>
				<td class="w640" height="21" width="800" style="font-size: 21px; line-height: 21px;">&nbsp;</td>
			</tr>

		</tbody>
	</table>
</layout>